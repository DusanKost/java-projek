<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserController</servlet-name>
        <servlet-class>controllers.UserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProizvodController</servlet-name>
        <servlet-class>controllers.ProizvodController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controllers.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KupljenProizvodController</servlet-name>
        <servlet-class>controllers.KupljenProizvodController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KorpaController</servlet-name>
        <servlet-class>controllers.KorpaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRm1Controllerr</servlet-name>
        <servlet-class>controllers.AddRm1Controllerr</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserController</servlet-name>
        <url-pattern>/UserController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProizvodController</servlet-name>
        <url-pattern>/ProizvodController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KupljenProizvodController</servlet-name>
        <url-pattern>/KupljenProizvodController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KorpaController</servlet-name>
        <url-pattern>/KorpaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRm1Controllerr</servlet-name>
        <url-pattern>/AddRm1Controllerr</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
